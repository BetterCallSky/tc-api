  "challengeCommunity": "develop",
  "challengeName": "Hercules PCDVR Android App - Standalone refactoring and Deleted Items album",
  "challengeType": "Assembly Competition",
  "checkpointSubmissionEndDate": "2014-05-24T09:19:00.000Z",
