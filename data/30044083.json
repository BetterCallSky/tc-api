  "finalSubmissionGuidelines": "<ul style=\"list-style-type:square\">\n\t<li>\n\t<p><span style=\"font-size:12px\"><span style=\"font-family:arial,helvetica,sans-serif\">MEAN stack only.&nbsp;</span></span></p>\n\t</li>\n\t<li>\n\t<p><span style=\"font-size:12px\"><span style=\"font-family:arial,helvetica,sans-serif\"><span style=\"background-color:transparent; color:rgb(0, 0, 0)\">Upload all your source code as a zip</span></span></span></p>\n\t</li>\n\t<li>\n\t<p><span style=\"font-size:12px\"><span style=\"font-family:arial,helvetica,sans-serif\"><span style=\"background-color:transparent; color:rgb(0, 0, 0)\">upload documentation for how to run your submission.</span></span></span></p>\n\t</li>\n\t<li>\n\t<p><span style=\"font-size:12px\"><span style=\"font-family:arial,helvetica,sans-serif\"><span>provide a video overview of your submission</span></span></span></p>\n\t</li>\n</ul>\n",
