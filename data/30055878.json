  "challengeCommunity": "develop",
  "challengeName": "Drone Series - DronePostion controller,  service and swagger update",
  "challengeType": "First2Finish",
  "checkpointSubmissionEndDate": null,
  "currentPhaseEndDate": "2016-12-10T03:24:00.000Z",
