  "challengeCommunity": "develop",
  "challengeName": "FinancialForce - Google App Script Plugin - Single vs Batch statements",
  "challengeType": "First2Finish",
  "checkpointSubmissionEndDate": null,
  "currentPhaseEndDate": "2016-04-15T05:00:00.000Z",
