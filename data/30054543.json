  "finalSubmissionGuidelines": "<span style=\"font-family:sofia pro,merriweather sans,helvetica,arial,sans-serif; font-size:14px\">Submit the following</span><br />\n<br />\n<span style=\"font-family:sofia pro,merriweather sans,helvetica,arial,sans-serif; font-size:14px\">- Source code zip</span><br />\n<br />\n<span style=\"font-family:sofia pro,merriweather sans,helvetica,arial,sans-serif; font-size:14px\">- Unmanaged package link</span><br />\n<br />\n<span style=\"font-family:sofia pro,merriweather sans,helvetica,arial,sans-serif; font-size:14px\">- Demo video (mandatory)</span><br />\n<br />\n<span style=\"font-family:sofia pro,merriweather sans,helvetica,arial,sans-serif; font-size:14px\">- Deployment Guide (mandatory)</span><br />\n<br />\n<span style=\"font-family:sofia pro,merriweather sans,helvetica,arial,sans-serif; font-size:14px\">- Demo org credentials (optional but highly recommended)</span>",
